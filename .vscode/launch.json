{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug-DAP",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}\\build\\basic_framework.elf",
      "request": "launch",
      "type": "cortex-debug",
      "device": "STM32F407IG",
      "svdFile": "STM32F407.svd",
      "servertype": "openocd",
      "configFiles": [
        "openocd_dap.cfg"
      ],
      "runToEntryPoint": "main"
    },
    {
      "name": "Debug-Jlink",
      "cwd": "${workspaceFolder}",
      "executable": "${workspaceRoot}\\build\\basic_framework.elf",
      "request": "launch",
      "type": "cortex-debug",
      "device": "STM32F407IG",
      "runToEntryPoint": "main",
      "showDevDebugOutput": "none",
      "servertype": "jlink",
      "interface": "swd",
      "svdFile": "STM32F407.svd",
      "rtos": "FreeRTOS"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "d:/Users/23532/Desktop/balance_infantry/WANGONG/application",
      "program": "d:/Users/23532/Desktop/balance_infantry/WANGONG/application/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
